@import "../../wxSortPickerView/wxSortPickerView.wxss";
.search {
  padding: 15rpx 60rpx;
  width: 100%;
  height: 90rpx;

  box-sizing: border-box;
  background-color: #eee;
  border-bottom: 3rpx solid #ccc;
}
.search > input {
  width: 100%;
  height: 60rpx;
  border-radius: 8rpx;
  background-color: #fff;
  font-size: 24rpx;
}
.txt {
  text-align: center;
  line-height: 60rpx;
  color: #999;
  font-size: 24rpx;
}




